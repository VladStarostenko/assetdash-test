{
  "name": "assetdash",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "dev:watch": "nodemon --watch 'src/**/*.{js,ts}' --exec 'ts-node -T' src/index.ts",
    "dev:start": "NODE_ENV=dev ts-node src/index.ts",
    "lint": "eslint '{src,test}/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "ts-node": "^8.6.2",
    "knex": "^0.20.11",
    "pg": "^8.2.1",
    "@restless/restless": "^0.4.2",
    "@restless/sanitizers": "^0.2.4",
    "fast-csv": "^4.3.0"
  }
}
